<AddIn name        = "VimSD"
       author      = "Arkadius Gil"
       description = "Vi Keybindings for SharpDevelop">
	
	<Manifest>
		<Identity name = "ViSD"/>
	</Manifest>
	
	<Runtime>
		<Import assembly = "ViSD.dll"/>
	</Runtime>
	
	<Path name="/SharpDevelop/Workbench/LanguageBindings">
		<Class id="Vimsd" class="ViSD.Visd"/>
	</Path>
	
--></AddIn>
